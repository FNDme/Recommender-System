{
  "exclude": [
    "test",
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "target": "ES6",
    // "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  }
}
